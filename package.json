{
  "name": "lissetteibnz.github.io-react",
  "version": "1.0.0",
  "description": "A blog site",
  "main": "index.js",
  "scripts": {
    "start": "run-p -l type-check:watch start:dev",
    "start:dev": "webpack-dev-server --config ./config/webpack/dev.js",
    "build": "run-p -l type-check build:prod",
    "build:prod": "npm run clean && webpack --config ./config/webpack/prod.js",
    "analyze": "npm run clean && npm run type-check && webpack --config ./config/webpack/analyze.js",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "test": "jest -c ./config/test/jest.js --verbose",
    "test:watch": "npm run test -- --watchAll -i",
    "clean": "rimraf dist && rimraf index.html",
    "lint": "eslint src --ext .ts,.tsx ",
    "lint:fix": "npm run lint -- --fix"
  },
  "author": "Sara Lissette",
  "license": "MIT",
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "apollo-boost": "^0.4.7",
    "emotion": "^10.0.27",
    "graphql": "^14.6.0",
    "highlight.js": "^9.18.1",
    "markdown-it": "^10.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.20",
    "react-router-dom": "^5.1.2",
    "reading-time": "^1.2.0",
    "regenerator-runtime": "^0.13.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@hot-loader/react-dom": "^16.13.0",
    "@types/highlightjs": "^9.12.0",
    "@types/jest": "^25.1.4",
    "@types/markdown-it": "0.0.9",
    "@types/react": "^16.9.26",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^5.1.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-emotion": "^10.0.29",
    "css-loader": "^3.4.2",
    "dotenv-webpack": "^1.7.0",
    "html-loader": "^1.0.0",
    "html-webpack-plugin": "^4.0.2",
    "jest": "^25.2.4",
    "markdown-loader": "^5.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "style-loader": "^1.1.3",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npm run lint:fix",
      "pretty-quick â€” staged",
      "git add"
    ]
  }
}
